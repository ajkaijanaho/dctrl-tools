#~ msgstr "Test auf numerische Übereinstimmung."

#~ msgid "Numerical test: <."
#~ msgstr "Numerischer Test: <."

#~ msgid "Numerical test: <=."
#~ msgstr "Numerischer Test: <=."

#~ msgid "Numerical test: >."
#~ msgstr "Numerischer Test: >."

#~ msgid "Numerical test: >=."
#~ msgstr "Numerischer Test: >=."

